@using ObCore.Models
@{
	ViewBag.Title = "Home Page";
	//List<Notification> notifications = ViewBag.Notifications;
	//List<Notification> messages = ViewBag.Messages;
	IEnumerable<Notification> notifications = ViewBag.Notifications;
	IEnumerable<Notification> messages = ViewBag.Messages;
}
<h2>@ViewBag.Message</h2>
<div class="tabbable">
	<!-- Only required for left/right tabs -->
	<ul class="nav nav-tabs">
		<li class="active"><a href="#AllNotificationsTab" data-toggle="tab">All Notifications</a></li>
		<li><a href="#MessagesTab" data-toggle="tab">Messages</a></li>
		<li><a href="#PhoneListTab" data-toggle="tab">Phone List</a></li>
	</ul>
<div class="tab-content">
	<div class="tab-pane active" id="AllNotificationsTab">
		<table class=" table table-striped table-condensed notifications">
			@foreach (var item in notifications) {
				@Html.Partial("_Notification", item);
			}
		</table>
	</div>
	<div class="tab-pane" id="MessagesTab">
		<table class=" table table-striped table-condensed notifications">
			@foreach (var item in messages) {
				@Html.Partial("_Notification", item);
			}
		</table>
	</div>
	<div class="tab-pane" id="PhoneListTab">
		<p>Phone numbers go here.</p>
	</div>
</div>
</div>
