@using ObCore.Models
@{
	ViewBag.Title = "Home Page";
	//List<Notification> notifications = ViewBag.Notifications;
	//List<Notification> messages = ViewBag.Messages;
	IEnumerable<Notification> notifications = ViewBag.Notifications;
	IEnumerable<Notification> messages = ViewBag.Messages;
}
<h2>@ViewBag.Message</h2>
<h2>Notifications</h2>
<h2>Homie</h2>
<table class=" table table-striped table-condensed">
	@foreach (var item in notifications) {
		@Html.Partial("_Notification", item);
	}
</table>
<h2>Messages and Comments</h2>
<table class=" table table-striped table-condensed">
	@foreach (var item in messages) {
		@Html.Partial("_Notification", item);
	}
</table>
